    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "rollup": "^2.26.5",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.3",
